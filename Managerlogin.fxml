<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="414.0" prefWidth="590.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ManagerloginController">
   <children>
      <Pane prefHeight="112.0" prefWidth="628.0" style="-fx-background-color: orange;">
         <children>
            <Label layoutX="248.0" layoutY="23.0" prefHeight="66.0" prefWidth="133.0" text="   Login" textAlignment="RIGHT" textFill="WHITE" wrapText="true">
               <font>
                  <Font name="Arial Bold" size="31.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Label layoutX="149.0" layoutY="172.0" text="Username">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="270.0" layoutY="172.0" prefHeight="28.0" prefWidth="199.0" promptText="Username" />
      <Label layoutX="151.0" layoutY="231.0" text="Password">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordTextField" layoutX="270.0" layoutY="231.0" prefHeight="28.0" prefWidth="199.0" promptText="Password" />
      <Button fx:id="loginButton" layoutX="347.0" layoutY="322.0" mnemonicParsing="false" onAction="#checkBlank" prefHeight="28.0" prefWidth="79.0" text="Login">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="cancelButton" layoutX="210.0" layoutY="322.0" mnemonicParsing="false" onAction="#cancelButtonOnAction" prefHeight="28.0" prefWidth="79.0" text="Cancel">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Label fx:id="messageLabel" alignment="CENTER" layoutX="155.0" layoutY="281.0" prefHeight="28.0" prefWidth="318.0" textFill="RED">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
