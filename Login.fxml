<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="414.0" prefWidth="590.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
   <children>
      <AnchorPane prefHeight="98.0" prefWidth="628.0" style="-fx-background-color: orange;">
         <children>
            <Label alignment="CENTER" layoutX="247.0" layoutY="16.0" prefHeight="66.0" prefWidth="133.0" text="   Login" textAlignment="RIGHT" textFill="WHITE" wrapText="true">
               <font>
                  <Font name="Arial Bold" size="31.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Label layoutX="162.0" layoutY="162.0" text="Username">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label layoutX="164.0" layoutY="221.0" text="Password">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="293.0" layoutY="162.0" prefHeight="28.0" prefWidth="199.0" promptText="Username" />
      <PasswordField fx:id="passwordTextField" layoutX="293.0" layoutY="221.0" prefHeight="28.0" prefWidth="199.0" promptText="Password" />
      <Button fx:id="loginButton" layoutX="353.0" layoutY="360.0" mnemonicParsing="false" onAction="#checkBlank" prefHeight="28.0" prefWidth="79.0" text="Login">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="cancelButton" layoutX="207.0" layoutY="360.0" mnemonicParsing="false" onAction="#cancelButtonOnAction" prefHeight="28.0" prefWidth="79.0" text="Cancel">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="createAccountButton" layoutX="174.0" layoutY="318.0" mnemonicParsing="false" onAction="#createAccountButtonOnAction" prefHeight="26.0" prefWidth="310.0" text="Create an Account" />
      <Label fx:id="messageLabel" alignment="CENTER" layoutX="152.0" layoutY="277.0" prefHeight="18.0" prefWidth="353.0" textAlignment="CENTER" textFill="#ff0303">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
